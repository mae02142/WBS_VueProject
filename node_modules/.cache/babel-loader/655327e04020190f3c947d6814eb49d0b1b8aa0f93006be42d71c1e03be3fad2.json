{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport SideBar from './components/SideBar';\nimport GanttContainer from \"./components/GanttContainer.vue\";\nimport FullCalendar from \"./components/FullCalendar.vue\";\nimport axios from \"axios\";\nimport mixin from \"./mixin\";\nexport default {\n  components: {\n    SideBar,\n    GanttContainer,\n    FullCalendar\n  },\n  inject: [\"eventBus\"],\n  mixins: [mixin],\n  data() {\n    return {\n      todoList: [],\n      todoCalendar: [],\n      todoTimeLine: [],\n      isTimeLine: true,\n      isCalendar: false\n    };\n  },\n  mounted() {\n    this.eventBus.on('getTodoList', project => {\n      this.getTodoList(project);\n    });\n  },\n  beforeUnmount() {\n    this.eventBus.off('getTodoList');\n  },\n  methods: {\n    getTodoList(project) {\n      axios.post(\"http://localhost:8030/api/getAllTodoList\", {\n        project_num: project.project_num,\n        t_key: this.key\n      }).then(response => {\n        this.todoList = response.data;\n        console.log(\"Get Todo data >>>>\", this.todoList);\n        this.todoListforCal();\n      }).catch(error => {\n        console.log(\"Failed to Get Todo List >>>>\", error);\n      });\n    },\n    // getTodoList()\n\n    todoListforCal() {\n      var dataList = this.todoList;\n      dataList.map(element => {\n        const startDate = new Date(element.start_date);\n        startDate.setHours(0, 0, 0, 0);\n        const endDate = new Date(element.due_date);\n        endDate.setHours(0, 0, 0, 0);\n        startDate.setDate(startDate.getDate() + 1);\n        endDate.setDate(endDate.getDate() + 1);\n        this.todoCalendar.push({\n          id: element.todo_num,\n          title: element.todo_title,\n          start: startDate.toISOString().split('T')[0],\n          end: endDate.toISOString().split('T')[0],\n          member_name: element.member_name,\n          content: element.content,\n          member_num: element.member_num\n        });\n      });\n      console.log(\"todo Calendar >>>>\", this.todoCalendar);\n    },\n    todoListforTL() {},\n    toggleView() {\n      // toggle for category\n      this.isTimeLine = !this.isTimeLine;\n      this.isCalendar = !this.isCalendar;\n    }\n  }\n};","map":{"version":3,"names":["SideBar","GanttContainer","FullCalendar","axios","mixin","components","inject","mixins","data","todoList","todoCalendar","todoTimeLine","isTimeLine","isCalendar","mounted","eventBus","on","project","getTodoList","beforeUnmount","off","methods","post","project_num","t_key","key","then","response","console","log","todoListforCal","catch","error","dataList","map","element","startDate","Date","start_date","setHours","endDate","due_date","setDate","getDate","push","id","todo_num","title","todo_title","start","toISOString","split","end","member_name","content","member_num","todoListforTL","toggleView"],"sources":["C:\\vue_everyware\\WBS\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div class=\"tl-container\">\r\n    <SideBar @projectSelected=\"handleProjectSelected\"></SideBar>\r\n    <div class=\"tab\">\r\n      <span @click=\"toggleView()\">타임라인</span>\r\n      <span @click=\"toggleView()\">캘린더</span>\r\n      <GanttContainer v-if=\"isTimeLine\"\r\n        :todoList=\"todoTimeLine\">\r\n      </GanttContainer>\r\n      <FullCalendar v-if=\"isCalendar\"\r\n        :todoList=\"todoCalendar\">\r\n      </FullCalendar>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport SideBar from './components/SideBar';\r\nimport GanttContainer from \"./components/GanttContainer.vue\";\r\nimport FullCalendar from \"./components/FullCalendar.vue\";\r\nimport axios from \"axios\";\r\nimport mixin from \"./mixin\";\r\n\r\nexport default {\r\n  components: { SideBar, GanttContainer, FullCalendar},\r\n  inject: [\"eventBus\"],\r\n  mixins:[mixin],\r\n  data() {\r\n    return {\r\n      todoList: [],\r\n      todoCalendar: [],\r\n      todoTimeLine: [],\r\n      isTimeLine: true,\r\n      isCalendar: false,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.eventBus.on('getTodoList',(project)=>{\r\n      this.getTodoList(project);\r\n    })\r\n  },\r\n  beforeUnmount() {\r\n    this.eventBus.off('getTodoList');\r\n  },\r\n  methods: {\r\n    getTodoList(project) {\r\n      axios.post(\"http://localhost:8030/api/getAllTodoList\", {\r\n        project_num: project.project_num,\r\n        t_key: this.key\r\n      })\r\n      .then(response => {\r\n        this.todoList = response.data;\r\n        console.log(\"Get Todo data >>>>\", this.todoList);\r\n        this.todoListforCal();\r\n      })\r\n      .catch (error => {\r\n        console.log(\"Failed to Get Todo List >>>>\", error);\r\n      })\r\n    }, // getTodoList()\r\n\r\n    todoListforCal() {\r\n      var dataList = this.todoList;\r\n      dataList.map((element) => {\r\n        const startDate = new Date(element.start_date);\r\n        startDate.setHours(0, 0, 0, 0);\r\n        const endDate = new Date(element.due_date);\r\n        endDate.setHours(0, 0, 0, 0);\r\n        startDate.setDate(startDate.getDate() + 1);\r\n        endDate.setDate(endDate.getDate() + 1);\r\n\r\n        this.todoCalendar.push({\r\n          id: element.todo_num,\r\n          title: element.todo_title,\r\n          start: startDate.toISOString().split('T')[0],\r\n          end: endDate.toISOString().split('T')[0],\r\n          member_name: element.member_name,\r\n          content: element.content,\r\n          member_num: element.member_num \r\n        })\r\n      })\r\n      console.log(\"todo Calendar >>>>\", this.todoCalendar)\r\n    },\r\n    todoListforTL() {\r\n\r\n    },\r\n    toggleView() { // toggle for category\r\n      this.isTimeLine = !this.isTimeLine;\r\n      this.isCalendar = !this.isCalendar;\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n\r\n<style>\r\n.tl-container{\r\n  margin-top:88px;\r\n}\r\n\r\n\r\n\r\n</style>\r\n"],"mappings":";AAiBA,OAAOA,OAAM,MAAO,sBAAsB;AAC1C,OAAOC,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,YAAW,MAAO,+