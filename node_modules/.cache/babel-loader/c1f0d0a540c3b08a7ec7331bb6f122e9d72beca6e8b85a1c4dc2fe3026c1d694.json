{"ast":null,"code":"import SideBar from './components/SideBar';\nimport WBSCategory from './components/WBSCategory.vue';\nimport GanttContainer from \"./components/GanttContainer.vue\";\nimport axios from \"axios\";\nimport mixin from \"./mixin\";\nexport default {\n  components: {\n    SideBar,\n    WBSCategory,\n    GanttContainer\n  },\n  inject: [\"eventBus\"],\n  mixins: [mixin],\n  data() {\n    return {\n      todoList: []\n    };\n  },\n  mounted() {\n    this.eventBus.on('getTodoList', project => {\n      this.getTodoList(project);\n    });\n  },\n  beforeUnmount() {\n    this.eventBus.off('getTodoList');\n  },\n  methods: {\n    getTodoList(project) {\n      console.log(\"eventBus emit project >>>>>\", project);\n      axios.post(\"http://localhost:8030/api/getAllTodoList\", {\n        project_num: project.project_num,\n        t_key: this.key\n      }).then(response => {\n        this.todoList = response.data;\n        console.log(\"Get Todo data >>>>\", this.todoList);\n      }).catch(error => {\n        console.log(\"Failed to Get Todo List >>>>\", error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["SideBar","WBSCategory","GanttContainer","axios","mixin","components","inject","mixins","data","todoList","mounted","eventBus","on","project","getTodoList","beforeUnmount","off","methods","console","log","post","project_num","t_key","key","then","response","catch","error"],"sources":["C:\\vue_everyware\\WBS\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div class=\"tl-container\">\r\n    <SideBar @projectSelected=\"handleProjectSelected\"></SideBar>\r\n    <div class=\"gantt-container\">\r\n      <WBSCategory ></WBSCategory>\r\n      <GanttContainer :projectList=\"projectList\"></GanttContainer>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport SideBar from './components/SideBar';\r\nimport WBSCategory from './components/WBSCategory.vue';\r\nimport GanttContainer from \"./components/GanttContainer.vue\";\r\nimport axios from \"axios\";\r\nimport mixin from \"./mixin\";\r\n\r\nexport default {\r\n  components: { SideBar, WBSCategory, GanttContainer},\r\n  inject: [\"eventBus\"],\r\n  mixins:[mixin],\r\n  data() {\r\n    return {\r\n      todoList: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.eventBus.on('getTodoList',(project)=>{\r\n      this.getTodoList(project);\r\n    })\r\n  },\r\n  beforeUnmount() {\r\n    this.eventBus.off('getTodoList');\r\n  },\r\n  methods: {\r\n    getTodoList(project){\r\n      console.log(\"eventBus emit project >>>>>\", project);\r\n      axios.post(\"http://localhost:8030/api/getAllTodoList\", {\r\n        project_num: project.project_num,\r\n        t_key: this.key\r\n      })\r\n      .then(response => {\r\n        this.todoList = response.data;\r\n        console.log(\"Get Todo data >>>>\", this.todoList)\r\n      })\r\n      .catch (error => {\r\n        console.log(\"Failed to Get Todo List >>>>\", error);\r\n      })\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n\r\n<style>\r\n.tl-container{\r\n  margin-top:88px;\r\n}\r\n\r\n\r\n\r\n</style>\r\n"],"mappings":"AAWA,OAAOA,OAAM,MAAO,sBAAsB;AAC1C,OAAOC,WAAU,MAAO,8BAA8B;AACtD,OAAOC,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,KAAI,MAAO,SAAS;AAE3B,eAAe;EACbC,UAAU,EAAE;IAAEL,OAAO;IAAEC,WAAW;IAAEC;EAAc,CAAC;EACnDI,MAAM,EAAE,CAAC,UAAU,CAAC;EACpBC,MAAM,EAAC,CAACH,KAAK,CAAC;EACdI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,QAAQ,CAACC,EAAE,CAAC,aAAa,EAAEC,OAAO,IAAG;MACxC,IAAI,CAACC,WAAW,CAACD,OAAO,CAAC;IAC3B,CAAC;EACH,CAAC;EACDE,aAAaA,CAAA,EAAG;IACd,IAAI,CAACJ,QAAQ,CAACK,GAAG,CAAC,aAAa,CAAC;EAClC,CAAC;EACDC,OAAO,EAAE;IACPH,WAAWA,CAACD,OAAO,EAAC;MAClBK,OAAO,CAACC,GAAG,CAAC,6BAA6B,EAAEN,OAAO,CAAC;MACnDV,KAAK,CAACiB,IAAI,CAAC,0CAA0C,EAAE;QACrDC,WAAW,EAAER,OAAO,CAACQ,WAAW;QAChCC,KAAK,EAAE,IAAI,CAACC;MACd,CAAC,EACAC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAAChB,QAAO,GAAIgB,QAAQ,CAACjB,IAAI;QAC7BU,OAAO,CAACC,GAAG,CAAC,oBAAoB,EAAE,IAAI,CAACV,QAAQ;MACjD,C