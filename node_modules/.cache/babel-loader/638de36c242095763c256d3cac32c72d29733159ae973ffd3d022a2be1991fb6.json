{"ast":null,"code":"import SideBar from './components/SideBar';\nimport GanttContainer from \"./components/GanttContainer.vue\";\nimport FullCalendar from \"./components/FullCalendar.vue\";\nimport axios from \"axios\";\nimport mixin from \"./mixin\";\nexport default {\n  components: {\n    SideBar,\n    GanttContainer,\n    FullCalendar\n  },\n  inject: [\"eventBus\"],\n  mixins: [mixin],\n  data() {\n    return {\n      todoList: [],\n      isTimeLine: true,\n      isCalendar: false\n    };\n  },\n  mounted() {\n    this.eventBus.on('getTodoList', project => {\n      this.getTodoList(project);\n    });\n  },\n  beforeUnmount() {\n    this.eventBus.off('getTodoList');\n  },\n  methods: {\n    getTodoList(project) {\n      console.log(\"eventBus emit project >>>>>\", project);\n      axios.post(\"http://localhost:8030/api/getAllTodoList\", {\n        project_num: project.project_num,\n        t_key: this.key\n      }).then(response => {\n        this.todoList = response.data;\n        console.log(\"Get Todo data >>>>\", this.todoList);\n      }).catch(error => {\n        console.log(\"Failed to Get Todo List >>>>\", error);\n      });\n    },\n    // getTodoList()\n\n    todoListforCal() {},\n    todoListforTL() {},\n    toggleView() {\n      if (this.isTimeLine) {\n        console.log(\"isTimeLine >>>\", this.isTimeLine);\n        this.isTimeLine = false;\n        console.log(\"isTimeLine >>>\", this.isTimeLine);\n      }\n      if (this.isCalendar) {\n        console.log(\"isCalendar >>>\", this.isCalendar);\n        this.isCalendar = false;\n        console.log(\"isCalendar >>>\", this.isCalendar);\n      }\n    }\n  }\n};","map":{"version":3,"names":["SideBar","GanttContainer","FullCalendar","axios","mixin","components","inject","mixins","data","todoList","isTimeLine","isCalendar","mounted","eventBus","on","project","getTodoList","beforeUnmount","off","methods","console","log","post","project_num","t_key","key","then","response","catch","error","todoListforCal",