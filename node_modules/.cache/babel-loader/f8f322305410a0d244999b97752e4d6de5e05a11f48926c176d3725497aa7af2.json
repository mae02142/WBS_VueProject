{"ast":null,"code":"import SideBar from './components/SideBar';\nimport GanttContainer from \"./components/GanttContainer.vue\";\nimport FullCalendar from \"./components/FullCalendar.vue\";\nimport axios from \"axios\";\nimport mixin from \"./mixin\";\nexport default {\n  components: {\n    SideBar,\n    GanttContainer,\n    FullCalendar\n  },\n  inject: [\"eventBus\"],\n  mixins: [mixin],\n  data() {\n    return {\n      todoList: [],\n      isTimeLine: true,\n      isCalendar: false\n    };\n  },\n  mounted() {\n    this.eventBus.on('getTodoList', project => {\n      this.getTodoList(project);\n    });\n  },\n  beforeUnmount() {\n    this.eventBus.off('getTodoList');\n  },\n  methods: {\n    getTodoList(project) {\n      axios.post(\"http://localhost:8030/api/getAllTodoList\", {\n        project_num: project.project_num,\n        t_key: this.key\n      }).then(response => {\n        this.todoList = response.data;\n        console.log(\"Get Todo data >>>>\", this.todoList);\n        this.eventBus.emit('todoCalendar', this.todoList);\n        //this.todoListforCal();\n      }).catch(error => {\n        console.log(\"Failed to Get Todo List >>>>\", error);\n      });\n    },\n    viewTimeline() {\n      if (!this.isTimeLine) {\n        this.isTimeLine = !this.isTimeLine;\n        this.isCalendar = false;\n      }\n    },\n    viewCalendar() {\n      if (!this.isCalendar) {\n        this.isCalendar = !this.isCalendar;\n        this.isTimeLine = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["SideBar","GanttContainer","FullCalendar","axios","mixin","components","inject","mixins","data","todoList","isTimeLine","isCalendar","mounted","eventBus","on","project","getTodoList","beforeUnmount","off","methods","post","project_num","t_key","key","then","response","console","log","emit","catch","error","viewTimeline","viewCalendar"],"sources":["C:\\vue_everyware\\WBS\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div class=\"tl-container\">\r\n    <SideBar @projectSelected=\"handleProjectSelected\"></SideBar>\r\n    <div class=\"tab\">\r\n      <button @click=\"viewTimeline()\">Time Line</button>\r\n      <button @click=\"viewCalendar()\">Calendar</button>\r\n      <GanttContainer v-if=\"isTimeLine\"\r\n        :todoList=\"todoList\">\r\n      </GanttContainer>\r\n      <FullCalendar v-if=\"isCalendar\"\r\n        :todoList=\"todoList\">\r\n      </FullCalendar>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport SideBar from './components/SideBar';\r\nimport GanttContainer from \"./components/GanttContainer.vue\";\r\nimport FullCalendar from \"./components/FullCalendar.vue\";\r\nimport axios from \"axios\";\r\nimport mixin from \"./mixin\";\r\n\r\nexport default {\r\n  components: { SideBar, GanttContainer, FullCalendar},\r\n  inject: [\"eventBus\"],\r\n  mixins:[mixin],\r\n  data() {\r\n    return {\r\n      todoList: [],\r\n      isTimeLine: true,\r\n      isCalendar: false,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.eventBus.on('getTodoList',(project)=>{\r\n      this.getTodoList(project);\r\n    })\r\n  },\r\n  beforeUnmount() {\r\n    this.eventBus.off('getTodoList');\r\n  },\r\n  methods: {\r\n    getTodoList(project) {\r\n      axios.post(\"http://localhost:8030/api/getAllTodoList\", {\r\n        project_num: project.project_num,\r\n        t_key: this.key\r\n      })\r\n      .then(response => {\r\n        this.todoList = response.data;\r\n        console.log(\"Get Todo data >>>>\", this.todoList);\r\n        this.eventBus.emit('todoCalendar',this.todoList);\r\n        //this.todoListforCal();\r\n      })\r\n      .catch (error => {\r\n        console.log(\"Failed to Get Todo List >>>>\", error);\r\n      })\r\n    },\r\n    viewTimeline() {\r\n      if (!this.isTimeLine) {\r\n        this.isTimeLine = !this.isTimeLine;\r\n        this.isCalendar = false;\r\n      }\r\n      \r\n    },\r\n    viewCalendar() {\r\n      if (!this.isCalendar) {\r\n        this.isCalendar = !this.isCalendar;\r\n        this.isTimeLine = false;\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style>\r\n.tl-container{\r\n  margin-top:88px;\r\n}\r\n\r\n\r\n\r\n</style>\r\n"],"mappings":"AAiBA,OAAOA,OAAM,MAAO,sBAAsB;AAC1C,OAAOC,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,YAAW,MAAO,+BAA+B;AACxD,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,KAAI,MAAO,SAAS;AAE3B,eAAe;EACbC,UAAU,EAAE;IAAEL,OAAO;IAAEC,cAAc;IAAEC;EAAY,CAAC;EACpDI,MAAM,EAAE,CAAC,UAAU,CAAC;EACpBC,MAAM,EAAC,CAACH,KAAK,CAAC;EACdI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE,IAAI;MAChBC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,QAAQ,CAACC,EAAE,CAAC,aAAa,EAAEC,OAAO,IAAG;MACxC,IAAI,CAACC,WAAW,CAACD,OAAO,CAAC;IAC3B,CAAC;EACH,CAAC;EACDE,aAAaA,CAAA,EAAG;IACd,IAAI,CAACJ,QAAQ,CAACK,GAAG,CAAC,aAAa,CAAC;EAClC,CAAC;EACDC,OAAO,EAAE;IACPH,WAAWA,CAACD,OAAO,EAAE;MACnBZ,KAAK,CAACiB,IAAI,CAAC,0CAA0C,EAAE;QACrDC,WAAW,EAAEN,OAAO,CAACM,WAAW;QAChCC,KAAK,EAAE,IAAI,CAACC;MACd,CAAC,EACAC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAAChB,QAAO,GAAIgB,QAAQ,CAACjB,IAAI;QAC7BkB,OAAO,CAACC,GAAG,CAAC,oBAAoB,EAAE,IAAI,CAAClB,QAAQ,CAAC;QAChD,IAAI,CAACI,QAAQ,CAACe,IAAI,CAAC,cAAc,EAAC,IAAI,CAACnB,QAAQ,CAAC;QAChD;MACF,CAAC,EACAoB,KAAI,CAAGC,KAAI,IAAK;QACfJ,OAAO,CAACC,GAAG,CAAC,8BAA8B,EAAEG,KAAK,CAAC;MACpD,CAAC;IACH,CAAC;IACDC,YAAYA,CAAA,EAAG;MACb,IAAI,CAAC,IAAI,CAACrB,UAAU,EAAE;QACpB,IAAI,CAACA,UAAS,GAAI,CAAC,IAAI,CAACA,UAAU;QAClC,IAAI,CAACC,UAAS,GAAI,KAAK;MACzB;IAEF,CAAC;IACDqB,YAAYA,CAAA,EAAG;MACb,IAAI,CAAC,IAAI,CAACrB,UAAU,EAAE;QACpB,IAAI,CAACA,UAAS,GAAI,CAAC,IAAI,CAACA,UAAU;QAClC,IAAI,CAACD,UAAS,GAAI,KAAK;MACzB;IACF;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}